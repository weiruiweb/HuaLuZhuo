<!--login.wxml-->
<view class="login">
  <image src="/images/login.png"></image>
</view>
<view id="login">
  <view class="login_input flexRowBetween">
    <view class="input_img">
      <image src="/images/login_phone.png"></image>
    </view>
     <view class="input_in">
      <input placeholder="请输入手机号"></input>
    </view>
  </view>
  <view class="login_input flexRowBetween">
    <view class="input_img">
      <image src="/images/login_pass.png"></image>
    </view>
     <view class="input_in" style="width:52%;">
      <input placeholder="请输入验证码"></input>
    </view>
    <view class="submit_info code_in" style="width:28.5%;">获取验证码</view>
  </view>
</view>
<view class="submit_info" style="width:93%;margin-top:160rpx;">
  登录
</view>
