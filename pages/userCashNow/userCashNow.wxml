<!--user.wxml-->
<view class="cashNow">
  <view class="p40">
    <view style="height:100rpx;line-height:100rpx;font-size:28rpx;color:#444;">请输入体现金额</view>
    <view class="flexRowBetween">
      <view style="width:91%;height:80rpx;border:solid 1px #ed7101;border-radius:10rpx;padding:0 2%;">
        <input style="height:80rpx;line-height:80rpx;font-size:30rpx;"></input>
      </view>
      <view style="font-size:24rpx;color:#666;width:5%;">元</view>
    </view>
  </view>
</view>
<view id="upload" style="margin-top:80rpx;">
  <view class="p40">
    <view class="uploder">
        <view class="uploder_all flexRowBetween">
         <!-- <block wx:for="{{web_imgData}}"> -->
            <view class="uploder_item" bindtap="upLoadImg" >
                <view class="uploader_input">
                  <image src="/images/cash_icon1.png" style="width:82rpx;height:82rpx;"></image>
                </view>
                <view class="upload_noe">上传身份证正面</view>
            </view>
          <!--</block>-->
           <view class="uploder_item" bindtap="upLoadImg" >
              <view class="uploader_input">
                <image src="/images/cash_icon1.png" style="width:82rpx;height:82rpx;"></image>
              </view>
              <view class="upload_noe">上传身份证反面</view>
            </view>


        </view>
    </view>
  </view>
</view>
<view class="confirm_bind"   bindtap="intoPathRedi" data-path="/pages/userCash/userCash">
  确认体现
</view>
